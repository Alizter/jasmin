/* Dummy program that features all available control structures */
inline
fn leaf(reg u64 a) -> reg u64 {
  reg u64 b;
  inline int i;
  b = 0;
  for i = 0 to 4 {
    b += a;
  }
  return b;
}

export
fn main(reg u64 x) -> reg u64 {
  reg u64 t r;
  reg bool pf;

  r = 0;

  while {
    t = 0;
    if (x >s 2) {
      t += 1;
    }
    if (x >s 3) {
      t += 2;
    } else {
      t += 3;
    }
    r += t;
  } (x >u 0) {
    r = leaf(r);
    x -= 1;
  }

  return r;
}
