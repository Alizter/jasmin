param int N = 4;

fn sum(reg u64[N] v) -> reg u64 {
  reg u64 r;
  inline int j;
  r = 0;
  for j = 0 to N {
    r += v[j];
  }
  return r;
}


export
fn main(reg u64 x) -> reg u64 {
  reg u64[N] t;
  reg u64 result;
  inline int i;
  for i = 0 to N {
    t[i] = x;
  }
  result = sum(t);
  return result;
}