extern fn add(zp, xp, yp) {
  /* n = limbs - 1 */

  for i in  0..n {
    x[i] = *(xp + i*8);
    y[i] = *(yp + i*8);
    if (i = 0) {
      cf? x[0] += y[0];
    } else {
      cf? x[i] += y[i] + cf;   
    }
  }

  add0 = 0;    
  add38 = 38;   
  add38 = 0 if !cf;

  for i in  0..n {
    if (i = 0) {
      cf? x[0] += add38;     
    } else {
      cf? x[i] += add0 + cf;
    }
  }

  add0 = add38 if cf;
  x[0] += add0;

  for j in 0..n {
    *(zp + j*8) = x[j];
  }
}
